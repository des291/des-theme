	"tokenColors": [
		{
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#FFFFFF59"
			}
		},
		{
			"scope": [
				"meta.keyword"
			],
			"settings": {
				"foreground": "#85AD99",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"meta.annotation",
				"meta.variable"
			],
			"settings": {
				"foreground": "#6C9380",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.accessor",
				"punctuation.section",
				"punctuation.definition",
				"punctuation.bracket",
				"punctuation.brackets",
				"keyword.operator.type.annotation",
				"meta.interface keyword.operator.optional",
				"meta.field.declaration keyword.operator.optional",
				"meta.brace"
			],
			"settings": {
				"foreground": "#FFFFFF66",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"meta.type.declaration entity.name.type",
				"entity.name.type.typedef"
			],
			"settings": {
				"foreground": "#F7AC6E",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"support.type",
				"meta.type.annotation entity.name.type",
				"source.go storage.type",
				"entity.other.inherited-class",
				"variable.other.object"
			],
			"settings": {
				"foreground": "#FFDBBD",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"keyword",
				"storage",
				"storage.type.keyword",
				"keyword.operator.new",
				"keyword.operator.delete"
			],
			"settings": {
				"foreground": "#94B3D1",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#FFC799",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"entity.name.function - meta.function-call",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#FFF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"meta.function-call entity.name.function",
				"meta.method-call entity.name.function",
				"meta.function-call variable.other.object",
				"meta.var.expr variable.other.object",
				"meta.function variable.other.object"
			],
			"settings": {
				"foreground": "#FFFFFFCC",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"string",
				"constant",
				"string punctuation.definition",
				"constant punctuation.definition"
			],
			"settings": {
				"foreground": "#94D1B3",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"string constant"
			],
			"settings": {
				"foreground": "#CCFFE6",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"string variable"
			],
			"settings": {
				"foreground": "#FFFFFFCC",
				"fontStyle": ""
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#3399FF1A"
			}
		},
		{
			"scope": "meta.tag",
			"settings": {
				"foreground": "#FFFFFF66"
			}
		},
		{
			"scope": [
				"meta.tag entity",
				"meta.tag.attributes"
			],
			"settings": {
				"foreground": "#FFFFFFCC"
			}
		},
		{
			"scope": "meta.tag entity.name",
			"settings": {
				"foreground": "#FFDBBD"
			}
		},
		{
			"scope": "meta.tag.metadata.doctype entity.name",
			"settings": {
				"foreground": "#94B3D1"
			}
		},
		{
			"scope": [
				"storage.register",
				"storage.other.register",
				"constant.language.registers",
				"entity.name.function.reg",
				"constant.language.register"
			],
			"settings": {
				"foreground": "#FFDBBD"
			}
		},
		{
			"scope": [
				"source.asm entity.directive",
				"source.asm support.function.directive"
			],
			"settings": {
				"foreground": "#FFFF99"
			}
		},
		{
			"scope": "text punctuation",
			"settings": {
				"foreground": "#FFFFFF66"
			}
		},
		{
			"scope": "text up.heading",
			"settings": {
				"foreground": "#FFF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "text markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "text markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "text markup.bold & markup.italic",
			"settings": {
				"fontStyle": "italic bold"
			}
		},
		{
			"scope": "text markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "text markup.list punctuation.definition.list_item",
			"settings": {
				"foreground": "#FFFF99",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "text.html.markdown meta.table.header",
			"settings": {
				"foreground": "#94D1B3",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"text.html.markdown punctuation.separator.table-cell",
				"text.html.markdown meta.table.header-separator punctuation.section.table-header"
			],
			"settings": {
				"foreground": "#94D1B3"
			}
		},
		{
			"scope": "text meta.link",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "text meta.link markup.underline.link",
			"settings": {
				"foreground": "#FFFFFF66"
			}
		},
		{
			"scope": "text meta.image",
			"settings": {
				"foreground": "#FFFFFF66"
			}
		},
		{
			"scope": "text meta.image markup.underline.link",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": [
				"text markup.raw",
				"text meta.code-fence.definition",
				"text markup.raw.code-fence"
			],
			"settings": {
				"foreground": "#FFFFFF12"
			}
		},
		{
			"scope": "text meta.code-fence.definition constant",
			"settings": {
				"foreground": "#FFFFFF66"
			}
		},
		{
			"scope": "source.diff",
			"settings": {
				"foreground": "#FFFFFF80"
			}
		},
		{
			"scope": "meta.diff",
			"settings": {
				"foreground": "#70C2C2",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#75B3F0",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#F7796E"
			}
		},
		{
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#3DF599"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#E48981",
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#9EFACC"
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#FF1500",
				"fontStyle": "italic underline"
			}
		},
		{
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#FF1500"
			}
		}
	]
}