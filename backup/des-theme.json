			}
		},
		{
			"name": "Decorator Objects",
			"scope": [
				"meta.decorator variable.other.object"
			],
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"punctuation.definition.keyword"
			],
			"settings": {
				"foreground": "#FFDBBD"
			}
		},
		{
			"name": "Keyword operator",
			"scope": [
